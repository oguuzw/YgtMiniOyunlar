{
  "version": 2,
  "builds": [
    {
      "src": "server/api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "docs/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/docs/$1"
    }
  ],
  "error": {
    "404": {
      "page": "/docs/404.html"
    }
  }
}
